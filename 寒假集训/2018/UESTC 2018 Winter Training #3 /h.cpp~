#include<iostream>
#include<cstdio>
#include<algorithm>
using namespace std;
int n;
int main(){
	scanf("%d",&n);
	printf("*");
	for (int i=2;i<n;i++)printf(" ");
	printf("* ");
	for (int i=1;i<=n;i++)printf("*");
	printf("\n");
	for (int j=2;j<n;j++){
		printf("*");
		for (int i=2;i<n;i++)printf(" ");
		printf("* ");	
		printf("*");
		for (int i=2;i<n;i++)printf(" ");
		printf("*\n");	
	}
	return 0;
}
